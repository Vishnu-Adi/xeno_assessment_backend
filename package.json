{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "PORT=3001 next dev --turbopack",
    "build": "next build --turbopack",
    "start": "PORT=3001 next start",
    "lint": "eslint",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "prisma db seed",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "seed": "chmod +x seed-data.sh && ./seed-data.sh seed",
    "seed:clean": "chmod +x seed-data.sh && ./seed-data.sh clean",
    "seed:tenant-a": "chmod +x seed-data.sh && ./seed-data.sh tenant-a",
    "seed:tenant-b": "chmod +x seed-data.sh && ./seed-data.sh tenant-b",
    "reset-db": "curl -X POST http://localhost:3000/api/admin/reset-tenant -H 'Content-Type: application/json' -d '{\"shop\":\"tenant-a-demo.myshopify.com\",\"mode\":\"all\"}' && curl -X POST http://localhost:3000/api/admin/reset-tenant -H 'Content-Type: application/json' -d '{\"shop\":\"tenant-b-demo.myshopify.com\",\"mode\":\"all\"}'",
    "create:orders": "chmod +x create-orders.sh && ./create-orders.sh orders",
    "create:carts": "chmod +x create-orders.sh && ./create-orders.sh carts",
    "create:both": "chmod +x create-orders.sh && ./create-orders.sh both",
    "show:variants": "chmod +x create-orders.sh && ./create-orders.sh variants",
    "backfill": "chmod +x comprehensive-backfill.sh && ./comprehensive-backfill.sh full",
    "backfill:clean": "chmod +x comprehensive-backfill.sh && ./comprehensive-backfill.sh clean",
    "backfill:customers": "chmod +x comprehensive-backfill.sh && ./comprehensive-backfill.sh customers",
    "backfill:carts": "chmod +x comprehensive-backfill.sh && ./comprehensive-backfill.sh carts",
    "realistic-data": "chmod +x simple-realistic-backfill.sh && ./simple-realistic-backfill.sh"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.87.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.543.0",
    "mysql2": "^3.14.5",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "nodemailer": "^7.0.6",
    "radix-ui": "^1.4.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.2.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^10.0.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.5.6",
    "prisma": "^6.15.0",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}